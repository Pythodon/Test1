if game.PlaceId == 18172550962 then
    wait(2)

    local args = { "Grasslands", "Nightmare", true }
    game.ReplicatedStorage.remotes.playerTP:FireServer(unpack(args))

    local label = Instance.new("TextLabel", Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui))
    label.Size = UDim2.new(0, 200, 0, 50)
    label.Position = UDim2.new(0.5, -100, 0, 20)
    label.BackgroundTransparency = 1
    label.TextScaled = true
    label.Font = Enum.Font.SourceSans
    label.TextColor3 = Color3.new(1, 0, 0)

    for i = 170, 0, -10 do
        label.Text = "Reset in: " .. i
        wait(10)
    end

    game.Players.LocalPlayer.Character:BreakJoints()
else
    loadstring(game:HttpGet("https://raw.githubusercontent.com/FrostByteV1/Scripts/refs/heads/main/PixelBlade"))()
end